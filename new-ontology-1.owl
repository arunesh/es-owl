<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#"
     xml:base="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1">
        <rdfs:comment xml:lang="en">This is an my first OWL 2.0 based ontology</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasBase -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasBase">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#isBaseOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasCountryOfOrigin -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasCountryOfOrigin">
        <rdfs:range rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#Country"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasIngredient -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasIngredient">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#isIngredientOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasSpiciness -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasSpiciness">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#SpicinessValuePartition"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasTopping -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasTopping">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#isToppingOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#PizzaTopping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#isBaseOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#isBaseOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#isIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#isIngredientOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#isToppingOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#isToppingOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#isIngredientOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#PizzaTopping"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#Pizza"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasCalorificContentValue -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasCalorificContentValue">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#AmericanaHotPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#AmericanaHotPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#JalapenoPepperTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#JalapenoPepperTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#PepperoniTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A pizza that has Mozarella, Tomato, pepperoni and Jalapeno toppings.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#AmericanaPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#AmericanaPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#PepperoniTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A pizza that has Mozarella, Tomato and pepperoni toppings.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#AnchovyTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#AnchovyTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#SeafoodTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#Medium"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#CaperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#CaperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#VegetableTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#Medium"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#CheeseTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#CheeseTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#PizzaTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#CheezyPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#CheezyPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasTopping"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#CheeseTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#Country -->

    <owl:Class rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#Country">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#America"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#England"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#France"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#Germany"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#Italy"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#DeepPanBase -->

    <owl:Class rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#DeepPanBase">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#PizzaBase"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#ThinAndCrispyBase"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#FourCheesePizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#FourCheesePizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasTopping"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">4</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#CheeseTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#GreenPepperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#GreenPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#PepperTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#Hot"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#HamTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#HamTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#MeatTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#Mild"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#HighCaloriePizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#HighCaloriePizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasCalorificContentValue"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:minInclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#Hot -->

    <owl:Class rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#Hot">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#SpicinessValuePartition"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#InterestingPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#InterestingPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasTopping"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">3</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#PizzaTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#JalapenoPepperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#JalapenoPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#PepperTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#Hot"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#LowCaloriePizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#LowCaloriePizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#hasCalorificContentValue"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:maxExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:maxExclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#MargheritaPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#MargheritaPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/arunesh/ontologies/2017/10/new-ontology-1#NamedPizza"/>
        <rdfs:subClassOf>
